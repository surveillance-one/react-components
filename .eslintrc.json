{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
        }
    },
    "rules": {
      "quotes": [
        2,
        "double"
      ],
      "indent": [
        1,
        "tab",
        {
          "SwitchCase": 1
        }
      ],
      "max-len": 0,
      "one-var": [
        2,
        {
          "uninitialized": "always",
          "initialized": "never"
        }
      ],
      "eqeqeq": 0,
      "camelcase": 0,
      "no-tabs": 0,
      "no-console": 0,
      "no-return-assign": 0,
      "no-constant-condition": 0,
      "vars-on-top": 0,
      "class-methods-use-this": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "no-plusplus": 0,
      "global-require": 0,
      "padded-blocks": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "brace-style": 0,
      "no-shadow": 0,
      "no-unused-vars": 0,
      "no-undef": 0,
      "function-paren-newline": 0,
      "consistent-return": 0,
      "no-bitwise": 0,
      "no-extra-boolean-cast": 0,
          "comma-dangle": 0
    }
  }